{
  "version": "0.19.0",
  "packages": [
    "packages/@css-blocks/*"
  ],
  "command": [
    "publish": [
      "allowBranch": "master",
      "conventionalCommits": true,
      "message": "chore(release): Publish %s"
      "conventionalCommits": false,
      "message": "error"
    ]
  ],
  "ignoreChanges": [
    "**/test/**",
    "**/*.md",
    "**/ts*.json",
    "**/*config*.json"
  ],
  "npmClient": "yarn",
  "useWorkspaces": true
}
